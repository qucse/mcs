<?xml version="1.0" encoding="UTF-8"?>
<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/sumoConfiguration.xsd">
  <input>
  <net-file value="../../maps/qatar_bus.net.xml" />
  <additional-files value="additional3.xml" />
  <route-files value="gen.routes.xml" />
  </input>
  <output>
    <write-license value="true" />
    <fcd-output value="fcd.xml" />
    <device.fcd.probability value="1" />
    <person-device.fcd.probability value="1" />
    <fcd-output.geo value="true"/>
  </output>

  <time>
    <end value="10000"/> <!-- 2 hours, 7200 seconds -->
  </time>


  <processing>
    <default.speeddev value="0" />
    <ignore-route-errors value="true" />
  </processing>

  <report>
    <duration-log.disable value="true" />
    <verbose value="true"/>
    <no-step-log value="true" />
    <log value="log.txt"/>
  </report>
</configuration>
